{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "dependencyDashboardTitle": "Renovate Dashboard",
  "dependencyDashboardApproval": true,
  "extends": [
    "config:recommended",
    ":disableRateLimiting",
    ":timezone(America/New_York)",
    ":semanticCommits",
    "config:recommended"
  ],
  "ansible-galaxy": {
    "fileMatch": [
      "(^|/)collections/.+\\.ya?ml(?:\\.j2)?$"
    ]
  },
  "pre-commit": {
    "enabled": true
  }
}
